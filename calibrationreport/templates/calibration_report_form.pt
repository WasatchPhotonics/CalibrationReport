

<!DOCTYPE html>
<html lang="${request.locale_name}">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Calibration Report web application</title>

        <!-- CSS -->
        <link rel="stylesheet" href="${request.static_url('calibrationreport:assets/css/pygments.css')}" type="text/css">
        <link rel="stylesheet" href="${request.static_url('calibrationreport:assets/css/form.css')}" type="text/css">
        <link rel="stylesheet" href="${request.static_url('calibrationreport:assets/css/bootstrap.css')}" type="text/css" media="screen" charset="utf-8">
        <link rel="stylesheet" href="${request.static_url('calibrationreport:assets/css/custom.css')}" type="text/css">

        <!-- JavaScript -->
        <script type="text/javascript" src="${request.static_url('calibrationreport:assets/js/jquery-2.0.3.min.js')}"></script>
        <script type="text/javascript" src="${request.static_url('calibrationreport:assets/js/bootstrap.min.js')}"></script>
        <script type="text/javascript" src="${request.static_url('calibrationreport:assets/js/deform.js')}"></script>
        <script type="text/javascript" src="${request.static_url('calibrationreport:assets/js/jquery.form-3.09.js')}"></script>
        <script type="text/javascript" src="${request.static_url('calibrationreport:assets/js/jquery.maskedinput-1.3.1.min.js')}"></script>
        
        <!-- faviconagraphy -->
        <link rel="apple-touch-icon" sizes="57x57" href="${request.static_url('calibrationreport:assets/img/apple-icon-57x57.png')}">
        <link rel="apple-touch-icon" sizes="60x60" href="${request.static_url('calibrationreport:assets/img/apple-icon-60x60.png')}">
        <link rel="apple-touch-icon" sizes="72x72" href="${request.static_url('calibrationreport:assets/img/apple-icon-72x72.png')}">
        <link rel="apple-touch-icon" sizes="76x76" href="${request.static_url('calibrationreport:assets/img/apple-icon-76x76.png')}">
        <link rel="apple-touch-icon" sizes="114x114" href="${request.static_url('calibrationreport:assets/img/apple-icon-114x114.png')}">
        <link rel="apple-touch-icon" sizes="120x120" href="${request.static_url('calibrationreport:assets/img/apple-icon-120x120.png')}">
        <link rel="apple-touch-icon" sizes="144x144" href="${request.static_url('calibrationreport:assets/img/apple-icon-144x144.png')}">
        <link rel="apple-touch-icon" sizes="152x152" href="${request.static_url('calibrationreport:assets/img/apple-icon-152x152.png')}">
        <link rel="apple-touch-icon" sizes="180x180" href="${request.static_url('calibrationreport:assets/img/apple-icon-180x180.png')}">
        <link rel="icon" type="image/png" sizes="192x192" href="${request.static_url('calibrationreport:assets/img/android-icon-192x192.png')}">
        <link rel="icon" type="image/png" sizes="32x32" href="${request.static_url('calibrationreport:assets/img/favicon-32x32.png')}">
        <link rel="icon" type="image/png" sizes="96x96" href="${request.static_url('calibrationreport:assets/img/favicon-96x96.png')}">
        <link rel="icon" type="image/png" sizes="16x16" href="${request.static_url('calibrationreport:assets/img/favicon-16x16.png')}">
        <link rel="manifest" href="${request.static_url('calibrationreport:assets/img/manifest.json')}">
        <meta name="msapplication-TileColor" content="#ffffff">
        <meta name="msapplication-TileImage" content="${request.static_url('calibrationreport:assets/img/ms-icon-144x144.png')}">
        <meta name="theme-color" content="#ffffff">
    </head>

    <body id="public">

    <div class="navbar navbar-default" role="navigation">
        <div class="container">
            <a class="navbar-brand" href="/">
                <img src="${request.static_url('calibrationreport:assets/img/favicon-32x32.png')}">
                Calibration Report Web Application
            </a>
        </div>
    </div>

    <div class="container">
        <div class="row">
            <div class="col-xs-4">
                <div class="panel panel-primary">
                    <div class="panel-heading">
                        <h3 class="panel-title">Enter parameters</h3>
                    </div>
                    <div class="panel-body">
                        <span tal:replace="structure form"/>
                        <BR>
                    </div>
                </div>
            </div>
            <div class="col-xs-6">
                <div class="panel panel-success">
                    <div class="panel-heading">
                        <h3 class="panel-title">Generated report</h3>
                    </div>
                    <div class="panel-body">
                        <div>
                            <a id="pdf_link"
href="/view_pdf/${data.serial}"><img class="responsive_img"
src="/view_thumbnail/${data.serial}"></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

        <script type="text/javascript">
            deform.load()
        </script>
    </body>
</html>
